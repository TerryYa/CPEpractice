#include <iostream>

using namespace std;

int gcd(int a,int b){
	if(!b)return a;
	else return gcd(b,a%b);
}

int sigma(int n){
	int G=0;
	for(int i=1;i<n;i++)
		for(int j=i+1;j<=n;j++){
		G+=gcd(i,j);
		}
	return G;
}

int main(){
	int n;
	while((cin >> n) && (n != 0)){
		cout << sigma(n) << endl;
	}
    return 0;
}
